{
  "builds": [
    { "src": "traffic-light/package.json", "use": "@vercel/node" },
    { "src": "whack-a-mole/package.json", "use": "@vercel/node" },
    { "src": "wordle-game/package.json", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/traffic-light(.*)", "dest": "/traffic-light" },
    { "src": "/whack-a-mole(.*)", "dest": "/whack-a-mole" },
    { "src": "/wordle-game(.*)", "dest": "/wordle-game" }
  ]
}
